abstract contract Context {
    function _msgSender() internal view virtual returns (address payable) {
        return msg.sender;
    }

    function _msgData() internal view virtual returns (bytes memory) {
        this; // silence state mutability warning without generating bytecode - see https://github.com/ethereum/solidity/issues/2691
        return msg.data;
    }
}

contract Ownable is Context {
    address private _owner;

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    /**
     * @dev Initializes the contract setting the deployer as the initial owner.
     */
    constructor () internal {
        address msgSender = _msgSender();
        _owner = msgSender;
        emit OwnershipTransferred(address(0), msgSender);
    }

    /**
     * @dev Returns the address of the current owner.
     */
    function owner() public view returns (address) {
        return _owner;
    }

    /**
     * @dev Throws if called by any account other than the owner.
     */
    modifier onlyOwner() {
        require(_owner == _msgSender(), "Ownable: caller is not the owner");
        _;
    }

    /**
     * @dev Leaves the contract without owner. It will not be possible to call
     * `onlyOwner` functions anymore. Can only be called by the current owner.
     *
     * NOTE: Renouncing ownership will leave the contract without an owner,
     * thereby removing any functionality that is only available to the owner.
     */
    function renounceOwnership() public virtual onlyOwner {
        emit OwnershipTransferred(_owner, address(0));
        _owner = address(0);
    }

    /**
     * @dev Transfers ownership of the contract to a new account (`newOwner`).
     * Can only be called by the current owner.
     */
    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        emit OwnershipTransferred(_owner, newOwner);
        _owner = newOwner;
    }
}
abstract contract IRewardDistributionRecipient is Ownable {
    address public rewardDistribution;

    function notifyRewardAmount(uint256 reward) external virtual;

    modifier onlyRewardDistribution() {
        require(
            _msgSender() == rewardDistribution,
            'Caller is not reward distribution'
        );
        _;
    }

    function setRewardDistribution(address _rewardDistribution)
        external
        virtual
        onlyOwner
    {
        rewardDistribution = _rewardDistribution;
    }
}

/**
 * @title LP质押合约
 */
contract LPTokenWrapper {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    /// @notice GoCash LP Token合约地址
    IERC20 public lpt;

    /// @dev 质押总量
    uint256 private _totalSupply;
    /// @dev 余额映射
    mapping(address => uint256) private _balances;

    /**
     * @dev 返回总量
     * @return 总量
     */
    function totalSupply() public view returns (uint256) {
        return _totalSupply;
    }
    /**
    * @dev Returns the balance of the account
    * @param account Account address
    * @return Balance
    */
    function balanceOf(address account) public view returns (uint256) {
        return _balances[account];
    }

    /**
    * @dev Stake shushi to Boardroom
    * @param amount Amount to stake
    */
    function stake(uint256 amount) public virtual {
        // Increase total supply
        _totalSupply = _totalSupply.add(amount);
        // Increase balance mapping
        _balances[msg.sender] = _balances[msg.sender].add(amount);
        // Transfer LPToken to current contract
        lpt.safeTransferFrom(msg.sender, address(this), amount);
    }

    /**
    * @dev Withdraw LPToken
    * @param amount Amount to withdraw
    */
    function withdraw(uint256 amount) public virtual {
        // Director's total stake amount
        uint256 directorShare = _balances[msg.sender];
        // Confirm director's total stake amount is greater than the withdrawal amount
        require(directorShare >= amount, 'withdraw request greater than staked amount');
        // Decrease total supply
        _totalSupply = _totalSupply.sub(amount);
        // Decrease balance
        _balances[msg.sender] = _balances[msg.sender].sub(amount);
        // Transfer LPToken to user
        lpt.safeTransfer(msg.sender, amount);
    }

}



     





/**
 * @title LP Token矿池合约
 * @notice 周期180天
 */
contract LPTokenPool is LPTokenWrapper, IRewardDistributionRecipient {
    IERC20 public token;
    /// @notice Duration of 180 days
    uint256 public DURATION = 180 days;
    /// @notice Start time
    uint256 public starttime; // starttime TBD
    /// @notice End time
    uint256 public periodFinish = 0;
    /// @notice Reward rate per second
    uint256 public rewardRate = 0;
    /// @notice Last update time
    uint256 public lastUpdateTime;
    /// @notice Stored reward amount
    uint256 public rewardPerTokenStored;
    /// @notice Payment of rewards per staked token for users
    mapping(address => uint256) public userRewardPerTokenPaid;
    /// @notice Unpaid reward amount for users
    mapping(address => uint256) public rewards;

    event RewardAdded(uint256 reward);
    event Staked(address indexed user, uint256 amount);
    event Withdrawn(address indexed user, uint256 amount);
    event RewardPaid(address indexed user, uint256 reward);

    /**

    @dev Constructor
    @param token_ Token address
    @param lptoken_ LPtoken address
    */
    constructor(address token_, address lptoken_ ) public payable{
        token = IERC20(token_);
        lpt = IERC20(lptoken_);
        starttime = block.timestamp;
    }
    /**

    @dev Check start time
    */
    modifier checkStart() {
         require(block.timestamp >= starttime, 'LPTokenSharePool: not start');
        _;
    }

    /**

    @dev Update reward
    @param account User address
    */
    modifier updateReward(address account) {
        // Stored reward amount = Reward per staked token
        rewardPerTokenStored = rewardPerToken();
        // Last update time = min(current time, end time)
        lastUpdateTime = lastTimeRewardApplicable();
        // If user address != 0 address
        if (account != address(0)) {
            // Unpaid reward amount for users = Earned user reward
            rewards[account] = earned(account);
            // Payment of rewards per staked token for users = Stored reward amount
            userRewardPerTokenPaid[account] = rewardPerTokenStored;
        }
        _;
    }


    /**

    @dev Returns the last deadline for rewards
    @return Last deadline
    @notice If the end time has not been reached, the current time is returned
    */
    function lastTimeRewardApplicable() public view returns (uint256) {
        // Minimum value(current time, end time)
        return Math.min(block.timestamp, periodFinish);
    }

    /**
    @dev Reward per staked token
    @return Reward amount
    */
    function rewardPerToken() public view returns (uint256) {
    // Return 0
    if (totalSupply() == 0) {
    return rewardPerTokenStored;
    }
    // Stored reward amount + (min(current time, end time) - Last update time) * Reward per second * 1e18 / Total supply
    return
        rewardPerTokenStored.add(
            lastTimeRewardApplicable()
                .sub(lastUpdateTime)
                .mul(rewardRate)
                .mul(1e18)
                .div(totalSupply())
        );
    }
    /**
    * @dev Returns the amount of rewards earned by an account.
    * @param account The address of the account to check.
    */
    function earned(address account) public view returns (uint256) {
        // The amount of rewards earned by the user is calculated as:
        // user's balance * (reward per token - user's rewards per token paid) / 1e18 + unreleased rewards
        return balanceOf(account)
            .mul(rewardPerToken().sub(userRewardPerTokenPaid[account]))
            .div(1e18)
            .add(rewards[account]);
    }

    /**
    * @dev Stakes a specified amount of tokens.
    * @param amount The amount of tokens to stake.
    */
    // stake visibility is public as overriding LPTokenWrapper's stake() function
    function stake(uint256 amount) public override updateReward(msg.sender) checkStart {
        // Make sure the amount is greater than 0.
        require(amount > 0, 'HUSDGOCLPTokenSharePool: Cannot stake 0');
        // Call the parent function to stake the tokens.
        super.stake(amount);
        // Emit a staked event.
        emit Staked(msg.sender, amount);
    }

    /**
    * @dev Withdraws a specified amount of staked tokens.
    * @param amount The amount of staked tokens to withdraw.
    */
    function withdraw(uint256 amount) public override updateReward(msg.sender) checkStart {
        // Make sure the amount is greater than 0.
        require(amount > 0, 'HUSDGOCLPTokenSharePool: Cannot withdraw 0');
        // Call the parent function to withdraw the tokens.
        super.withdraw(amount);
        // Emit a withdrawn event.
        emit Withdrawn(msg.sender, amount);
    }

    /**
     * @dev 退出
     */
    function exit() external {
        withdraw(balanceOf(msg.sender));
        getReward();
    }

    /**
     * @dev 获取奖励
     */
    function getReward() public updateReward(msg.sender) checkStart {
        // 奖励数量 = 用户已奖励的数量
        uint256 reward = earned(msg.sender);
        // 如果奖励数量>0
        if (reward > 0) {
            // 用户未发放的奖励数量 = 0
            rewards[msg.sender] = 0;
            // 发送奖励
            token.safeTransfer(msg.sender, reward);
            // 触发支付奖励事件
            emit RewardPaid(msg.sender, reward);
        }
    }


        /**

    @dev Notify reward amount
    @param reward The reward amount
    */
    function notifyRewardAmount(uint256 reward) external override onlyRewardDistribution updateReward(address(0)) {
    // If current time is after the start time
        if (block.timestamp > starttime) {
        // If current time is after the end time
            if (block.timestamp >= periodFinish) {
                // Reward rate is reward amount divided by 180 days
                rewardRate = reward.div(DURATION);
            } else {
                // Remaining time is the difference between end time and current time
                uint256 remaining = periodFinish.sub(block.timestamp);
                // Leftover reward amount is remaining time multiplied by reward rate (0 for the first execution)
                uint256 leftover = remaining.mul(rewardRate);
                // Reward rate is (reward amount + leftover reward amount) divided by 180 days
                rewardRate = reward.add(leftover).div(DURATION);
            }
            // Last update time is current time
            lastUpdateTime = block.timestamp;
            // End time is current time plus 180 days
            periodFinish = block.timestamp.add(DURATION);
            // Emit reward added event
            emit RewardAdded(reward);
        } else {
            // Reward rate is reward amount divided by 180 days
            rewardRate = reward.div(DURATION);
            // Last update time is start time
            lastUpdateTime = starttime;
            // End time is start time plus 180 days
            periodFinish = starttime.add(DURATION);
            // Emit reward added event
            emit RewardAdded(reward);
        }
        }
}


